{
  "While Statement": {
    "prefix": "while",
    "body": ["while (${1:condition}) {", "\t$0", "}"]
  },
  "Do-While Statement": {
    "prefix": "dow",
    "body": ["do {", "\t$0", "} while (${1:condition});"]
  },
  "Try-Catch Statement": {
    "prefix": "tryc",
    "body": ["try {", "$1", "} catch ($2) {", "$3", "}"]
  },
  "Set Timeout": {
    "prefix": "sett",
    "body": ["setTimeout(() => {", "$1", "}, $2);"]
  },
  "Set Timeout Empty": {
    "prefix": "sette",
    "body": ["setTimeout(() => {}, $1)"]
  },
  "Console Log": {
    "prefix": "clg",
    "body": ["console.log(\"$1\", $1);"]
  },
  "Console Log Empty": {
    "prefix": "clge",
    "body": ["console.log(\"$1\");"]
  }
}
